    "def num_link(a,b):\n",
    "    a = str(a).lstrip('0')  \n",
    "    b = str(b).lstrip('0')\n",
    "    len_c = len(a)+len(b)\n",
    "    c = ''\n",
    "    for i in range(len_c):\n",
    "        c += a[i%len(a)]\n",
    "        c += b[i%len(b)]\n",
    "    return c\n",
    "a = input()\n",
    "b = input()\n",
    "print(num_link(a,b))"
   ]
  }
